[$builddir/test_mat readvar $srcdir/matio_test_cases_compressed_le.mat var1], [0],
[[      Name: var1
      Rank: 2
Dimensions: 4 x 5
Class Type: Double Precision Array
 Data Type: IEEE 754 double-precision
{
1 5 9 13 17 @&t@
2 6 10 14 18 @&t@
3 7 11 15 19 @&t@
4 8 12 16 20 @&t@
}
]], [ignore])
